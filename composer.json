{
  "require": {
    "carlcs/craft-redactorcustomstyles": "3.0.4",
    "clubstudioltd/craft-asset-rev": "6.0.2",
    "craftcms/anchors": "2.3.0",
    "craftcms/cms": "3.6.17",
    "craftcms/contact-form": "2.2.7",
    "craftcms/redactor": "2.8.7",
    "doublesecretagency/craft-googlemaps": "4.0.8.1",
